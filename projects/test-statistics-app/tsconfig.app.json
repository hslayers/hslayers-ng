{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "target": "ES5",
    "paths": {
      "@angular/*": [ "node_modules/@angular/*" ],
      "rxjs/*": [ "node_modules/rxjs/*" ],
      "hslayers-ng": ["projects/hslayers/src/public-api"],
      "hslayers-ng/*": ["projects/hslayers/*", "projects/hslayers"],
      "ol/*": ["node_modules/ol/*"],
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
