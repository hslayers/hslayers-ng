{
  "name": "hslayers-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "node projects/hslayers/prebuild.js",
    "build": "ng build hslayers --prod",
    "prebuild-watch": "node projects/hslayers/prebuild.js",
    "build-watch": "ng build hslayers --watch",
    "release": "dotenv -- release-it",
    "find-circular": "npx madge --circular --extensions ts ./projects/hslayers/",
    "test": "ng test hslayers",
    "lint": "eslint --ignore-path .gitignore --fix-dry-run ./",
    "lint-stats": "eslint --ignore-path .gitignore --format node_modules/eslint-stats/byErrorAndWarning.js ./",
    "e2e": "ng e2e",
    "doc-hs": "cd ./projects/hslayers && npx compodoc -c .compodocrc.json -t",
    "doc-cesium": "cd ./projects/hslayers-cesium && npx compodoc -c .compodocrc.json -t",
    "doc-sensors": "cd ./projects/hslayers-sensors && npx compodoc -c .compodocrc.json -t",
    "doc": "npm run doc-hs && npm run doc-cesium && npm run doc-sensors && node scripts/gen-doc.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^10.2.4",
    "@angular/compiler": "^10.2.4",
    "@angular/core": "^10.2.4",
    "@angular/forms": "^10.2.4",
    "@angular/localize": "^10.2.4",
    "@angular/platform-browser": "^10.2.4",
    "@angular/platform-browser-dynamic": "^10.2.4",
    "@ng-bootstrap/ng-bootstrap": "^6.2.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^5.0.0",
    "@types/cesium": "^1.67.12",
    "bootstrap": "^4.6.0",
    "cesium": "^1.79.1",
    "deepmerge": "^4.2.2",
    "moment": "^2.29.1",
    "moment-interval": "^0.2.1",
    "ngx-cookie-service": "^10.0.0",
    "ol": "^6.5.0",
    "ol-popup": "^4.0.0",
    "proj4": "^2.7.2",
    "rxjs": "^6.6.6",
    "share-api-polyfill": "^1.0.20",
    "tslib": "^2.0.0",
    "vega": "5.19.1",
    "vega-embed": "6.15.2",
    "vega-lite": "^5.0.0",
    "xml-js": "^1.6.11",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~9.2.0",
    "@angular-eslint/eslint-plugin": "^2.0.1",
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular-devkit/build-ng-packagr": "~0.1002.3",
    "@angular-eslint/eslint-plugin-template": "^1.2.0",
    "@angular-eslint/template-parser": "^1.2.0",
    "@angular/cli": "~10.2.3",
    "@angular/compiler-cli": "~10.2.4",
    "@angularclass/hmr": "^3.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.6.6",
    "@types/jasminewd2": "~2.0.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.17.0",
    "codelyzer": "^6.0.1",
    "eslint": "^7.22.0",
    "eslint-config-openlayers": "^15.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-tsdoc": "^0.2.11",
    "eslint-stats": "^1.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "puppeteer": "^8.0.0",
    "ts-node": "~9.1.1",
    "typescript": "~4.0.7"
  }
}
