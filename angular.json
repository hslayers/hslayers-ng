{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "hslayers-ng": {
      "projectType": "application",
      "schematics": {},
      "root": "./",
      "sourceRoot": "./",
      "prefix": "",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "main": "main.ts",
            "tsConfig": "tsconfig.json",
            "polyfills": "polyfills.ts",
            "outputPath": "bundle/",
            "index": "hslayers.html"
          },
          "configurations": {
            "bundle": {
              "optimization": true,
              "sourceMap": false,
              "extractCss": false,
              "statsJson": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": false,
              "buildOptimizer": true,
              "tsConfig": "tsconfig.bundle.json"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hslayers-ng:build"
          }
        }
      }
    }
  },
  "defaultProject": "hslayers-ng",
  "cli": {
    "analytics": false
  }
}
