{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "hslayers-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "root": "",
      "sourceRoot": "projects/",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/hslayers-app",
            "index": "projects/hslayers-app/src/index.html",
            "main": "projects/hslayers-app/src/main.ts",
            "polyfills": "projects/hslayers-app/src/polyfills.ts",
            "tsConfig": "projects/hslayers-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/hslayers/assets",
                "output": "./assets"
              },
              {
                "glob": "README.md",
                "input": "projects/hslayers-app/",
                "output": "/"
              },
              {
                "glob": "package.json",
                "input": "projects/hslayers-app/",
                "output": "/"
              }
            ],
            "scripts": [],
            "customWebpackConfig": {
              "path": "projects/hslayers-app/custom-webpack.config.js"
            },
            "allowedCommonJsDependencies": [
              "xml-utils",
              "colormap",
              "lru-cache",
              "pbf",
              "queue",
              "xml-js",
              "geostyler-legend",
              "ol-popup",
              "geostyler-openlayers-parser",
              "geostyler-qgis-parser",
              "geostyler-sld-parser"
            ]
          },
          "configurations": {
            "bootstrap": {
              "fileReplacements": [
                {
                  "replace": "projects/hslayers-app/src/environments/environment.ts",
                  "with": "projects/hslayers-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "sourceMap": false,
              "namedChunks": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "200kb",
                  "maximumError": "300kb"
                }
              ],
              "styles": [
                "projects/hslayers/css/hslayers-ng.scss"
              ]
            },
            "wagtail": {
              "fileReplacements": [
                {
                  "replace": "projects/hslayers-app/src/environments/environment.ts",
                  "with": "projects/hslayers-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "sourceMap": false,
              "namedChunks": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "200kb",
                  "maximumError": "300kb"
                }
              ],
              "styles": [
                "dist/hslayers/css/hslayers-ng-wagtail.css"
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "15mb",
                  "maximumError": "20mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "200kb",
                  "maximumError": "300kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "hslayers-app:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "hslayers-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "hslayers-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/hslayers-app/test/test.ts",
            "polyfills": "projects/hslayers-app/src/polyfills.ts",
            "tsConfig": "projects/hslayers-app/tsconfig.spec.json",
            "karmaConfig": "projects/hslayers-app/karma.conf.js",
            "assets": [
              "projects/hslayers-app/src/favicon.ico",
              "projects/hslayers-app/src/assets"
            ],
            "styles": [
              "projects/hslayers-app/src/styles.sass"
            ],
            "scripts": []
          }
        }
      }
    },
